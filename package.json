{
  "name": "setup-igor",
  "author": "Shi Chen",
  "version": "0.0.27",
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build/lib/**/*",
    "build/index.*",
    "build/types/**/*"
  ],
  "scripts": {
    "release": "rimraf dist && ncc build src/index.ts -o dist",
    "test:main": "npm run release && node -r dotenv/config ./dist/index.js",
    "test": "npm run build && mocha --enable-source-maps --bail ./build/test/",
    "build": "rimraf build && tsc && tsc-alias",
    "version": "npm run format && git add -A",
    "postversion": "git push --follow-tags",
    "preversion": "npm run release"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@bscotch/gamemaker-releases": "^0.7.0",
    "adm-zip": "^0.5.10",
    "fs-extra": "^11.2.0",
    "plist": "^3.1.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/chai": "^4.3.12",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.25",
    "@types/plist": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vercel/ncc": "^0.38.1",
    "chai": "^5.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2"
  }
}
